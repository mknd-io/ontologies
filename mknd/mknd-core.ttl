#################################################################
#    Classes
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationRule
d3f:ApplicationRule rdf:type owl:Class .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#IPAddress
d3f:IPAddress rdf:type owl:Class ;
              rdfs:subClassOf :CommunicationProtocol ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty d3f:used-by ;
                                owl:someValuesFrom :Socket
                              ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#UserAccount
d3f:UserAccount rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:uses ;
                                  owl:someValuesFrom :Username
                                ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#UserAction
d3f:UserAction rdf:type owl:Class ;
               owl:disjointWith :MachineAction .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability
d3f:Vulnerability rdfs:subClassOf :MekaThing .


###  https://mekanoid.io/ontology/ASN
:ASN rdf:type owl:Class ;
     rdfs:subClassOf d3f:Identifier ,
                     :MekaThing ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :value ;
                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onDataRange xsd:string
                     ] ;
     rdfs:comment "Autonomous System Number"@en ;
     rdfs:label "ASN"@en .


###  https://mekanoid.io/ontology/Advisory
:Advisory rdf:type owl:Class ;
          rdfs:subClassOf d3f:Document ,
                          :MekaThing ;
          rdfs:isDefinedBy "https://csrc.nist.gov/glossary/term/advisory"^^xsd:anyURI ;
          rdfs:label "Advisory"@en ;
          skos:definition "Notification of significant new trends or developments regarding the threat to the information systems of an organization. This notification may include analytical insights into trends, intentions, technologies, or tactics of an adversary targeting information systems. Rationale: General definition of a commonly understood term."@en .


###  https://mekanoid.io/ontology/ApplicationUserAccount
:ApplicationUserAccount rdf:type owl:Class ;
                        rdfs:subClassOf d3f:UserAccount .


###  https://mekanoid.io/ontology/CVSS-RemediationLevel
:CVSS-RemediationLevel rdfs:label "Remediation Level" ;
                       skos:altLabel "RL" ;
                       skos:definition "The Remediation Level of a vulnerability is an important factor for prioritization. The typical vulnerability is unpatched when initially published. Workarounds or hotfixes may offer interim remediation until an official patch or upgrade is issued. Each of these respective stages adjusts the Temporal Score downwards, reflecting the decreasing urgency as remediation becomes final. The list of possible values is presented in Table 10. The less official and permanent a fix, the higher the vulnerability score." .


###  https://mekanoid.io/ontology/CVSS-ReportConfidence
:CVSS-ReportConfidence rdfs:label "Report Confidence" ;
                       skos:altLabel "RC" ;
                       skos:definition "This metric measures the degree of confidence in the existence of the vulnerability and the credibility of the known technical details. Sometimes only the existence of vulnerabilities is publicized, but without specific details. For example, an impact may be recognized as undesirable, but the root cause may not be known. The vulnerability may later be corroborated by research which suggests where the vulnerability may lie, though the research may not be certain. Finally, a vulnerability may be confirmed through acknowledgment by the author or vendor of the affected technology. The urgency of a vulnerability is higher when a vulnerability is known to exist with certainty. This metric also suggests the level of technical knowledge available to would-be attackers. The more a vulnerability is validated by the vendor or other reputable sources, the higher the score." .


###  https://mekanoid.io/ontology/CVSS-SeverityType
:CVSS-SeverityType rdfs:comment "Qualitative Severity Rating" ;
                   rdfs:label "Severity Type" ;
                   skos:definition "Specifies the CVSS base, temporal, threat, or environmental severity type." .


###  https://mekanoid.io/ontology/CommunicationProtocol
:CommunicationProtocol rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( :ICMP
                                                         :TCP
                                                         :UDP
                                                       )
                                           ] ;
                       rdfs:subClassOf :MekaThing ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty d3f:used-by ;
                                         owl:someValuesFrom d3f:NetworkPackets
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty d3f:used-by ;
                                         owl:someValuesFrom d3f:NetworkService
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty d3f:used-by ;
                                         owl:someValuesFrom d3f:NetworkSession
                                       ] ;
                       rdfs:isDefinedBy "https://en.wikipedia.org/wiki/Communication_protocol" .


###  https://mekanoid.io/ontology/Connection
:Connection rdf:type owl:Class ;
            rdfs:subClassOf :SystemObject .


###  https://mekanoid.io/ontology/Control
:Control rdf:type owl:Class ;
         rdfs:subClassOf :MekaThing .


###  https://mekanoid.io/ontology/CriticalVulnerability
:CriticalVulnerability owl:equivalentClass [ owl:intersectionOf ( d3f:Vulnerability
                                                                  [ rdf:type owl:Class ;
                                                                    owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :cvssBaseScore ;
                                                                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                owl:onDatatype xsd:decimal ;
                                                                                                                                owl:withRestrictions ( [ xsd:minInclusive 9.0
                                                                                                                                                       ]
                                                                                                                                                     )
                                                                                                                              ]
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :cvssBaseScore ;
                                                                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                owl:onDatatype xsd:decimal ;
                                                                                                                                owl:withRestrictions ( [ xsd:maxInclusive 10.0
                                                                                                                                                       ]
                                                                                                                                                     )
                                                                                                                              ]
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty :severityRating ;
                                                                                    owl:hasValue :CVSS-SeverityType-Critical
                                                                                  ]
                                                                                )
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf d3f:Vulnerability ;
                       rdfs:label "Critical Severity Vulnerability" .


###  https://mekanoid.io/ontology/Detection
:Detection rdf:type owl:Class ;
           rdfs:subClassOf d3f:DigitalEvent ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty d3f:created-by ;
                             owl:allValuesFrom :DetectionRule
                           ] ;
           rdfs:label "Finding" ;
           skos:altLabel "Alert"@en ,
                         "Detection"@en ,
                         "Signal"@en .


###  https://mekanoid.io/ontology/DetectionRule
:DetectionRule rdf:type owl:Class ;
               rdfs:subClassOf d3f:ApplicationRule ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty d3f:creates ;
                                 owl:someValuesFrom :Detection
                               ] ;
               rdfs:label "Detection Rule"@en .


###  https://mekanoid.io/ontology/EmailAddress
:EmailAddress rdf:type owl:Class ;
              rdfs:subClassOf :Observable .


###  https://mekanoid.io/ontology/EmailSenderString
:EmailSenderString rdf:type owl:Class ;
                   rdfs:subClassOf :Observable .


###  https://mekanoid.io/ontology/EmailSubjectString
:EmailSubjectString rdf:type owl:Class ;
                    rdfs:subClassOf :Observable .


###  https://mekanoid.io/ontology/FileName
:FileName rdf:type owl:Class ;
          rdfs:subClassOf :Observable ,
                          [ rdf:type owl:Restriction ;
