<https://mekanoid.io/ontology/> rdf:type owl:Ontology ;
                                 owl:imports <file:/Users/antong/Sandbox/Mekanoid/mknd-ontology/mknd/mknd-cvss.ttl> ,
                                             <file:/Users/antong/Sandbox/Mekanoid/mknd-ontology/mknd/mknd-observable.ttl> ,
                                             <http://d3fend.mitre.org/ontologies/d3fend/0.14.0/d3fend.owl> ;
                                 dcterms:description "Mekanoid Ontology (MKND) is a public ontology for semantic knowledge graph applications. It is defined with a purpose to make trusted and autonomous cybersecurity prevention and response possible. It is a superset of the MITRE D3FEND ontology, and is designed to be used in conjunction with it. Mekanoid is a work in progress, and is subject to change."@en ;
                                 dcterms:modified "2024-03-29"^^xsd:date ;
                                 dcterms:title "Mekanoid Ontology (MKND)" ;
                                 owl:versionInfo "0.3.3" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  https://mekanoid.io/ontology/context
:context rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://mekanoid.io/ontology/hasURL
:hasURL rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :reference ;
        rdfs:domain d3f:File ,
                    :Report .


###  https://mekanoid.io/ontology/id
:id rdf:type owl:ObjectProperty ,
             owl:InverseFunctionalProperty ;
    rdfs:domain :MekaThing .


###  https://mekanoid.io/ontology/importBucketUrl
:importBucketUrl rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasURL ;
                 rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/indicatedBy
:indicatedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :mayBeIndicatedBy ,
                                [ owl:inverseOf :mayIndicate
                                ] ;
             owl:inverseOf :indicates ;
             rdfs:label "indicated by"@en .


###  https://mekanoid.io/ontology/indicates
:indicates rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :mayIndicate ,
                              [ owl:inverseOf :mayBeIndicatedBy
                              ] ;
           rdfs:label "indicates"@en .


###  https://mekanoid.io/ontology/integrityImpact
:integrityImpact rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasCVSSMetric ;
                 rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/mayBeIndicatedBy
:mayBeIndicatedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf :mayIndicate ;
                  rdfs:label "may be indicated by"@en .


###  https://mekanoid.io/ontology/mayIndicate
:mayIndicate rdf:type owl:ObjectProperty ;
             rdfs:label "may indicate"@en .


###  https://mekanoid.io/ontology/mkndObjectProperty
:mkndObjectProperty rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :supports .


###  https://mekanoid.io/ontology/privilegesRequired
:privilegesRequired rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCVSSMetric ;
                    rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/reference
:reference rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty .


###  https://mekanoid.io/ontology/remediationLevel
:remediationLevel rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCVSSMetric ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:range :CVSS-RemediationLevel .


###  https://mekanoid.io/ontology/reportConfidence
:reportConfidence rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCVSSMetric ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:range :CVSS-ReportConfidence .


###  https://mekanoid.io/ontology/scope
:scope rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :hasCVSSMetric ;
       rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/severityRating
:severityRating rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasCVSSMetric ;
                rdf:type owl:FunctionalProperty ;
                rdfs:range :CVSS-SeverityType .


###  https://mekanoid.io/ontology/supports
:supports rdf:type owl:ObjectProperty ;
          rdfs:domain :Connection ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( d3f:SoftwareProduct
                                     d3f:SoftwareService
                                   )
                     ] ;
          rdfs:label "supports"@en ;
          skos:example "Connection x supports product or service y." .


###  https://mekanoid.io/ontology/updated-by
:updated-by rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf d3f:modified-by ;
            rdfs:label "updated by" .


###  https://mekanoid.io/ontology/userInteraction
:userInteraction rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasCVSSMetric ;
                 rdf:type owl:FunctionalProperty .


#################################################################
#    Data properties
#################################################################

###  https://mekanoid.io/ontology/cpe
:cpe rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf d3f:identifier ;
     rdfs:range xsd:string ;
     rdfs:label "CPE" ;
     skos:example "cpe:2.3:a:apple:icloud:1.0:*:*:*:*:macos:*:*" .


###  https://mekanoid.io/ontology/dateTime
:dateTime rdf:type owl:DatatypeProperty ;
          owl:equivalentProperty :timeSeen ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :MekaThing ;
          rdfs:range xsd:dateTimeStamp ;
          rdfs:label "has timestamp"@en ;
          skos:definition "A date and time associated with the entity"@en ;
          skos:example "2002-10-10T12:00:00-05:00" .


###  https://mekanoid.io/ontology/dateTimeEpoch
:dateTimeEpoch rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:range xsd:long ;
               rdfs:comment "A Unix epoch timestamp associated with the entity"@en ;
               rdfs:label "has epoch"@en ;
               skos:definition "A Unix epoch timestamp associated with the entity"@en ;
               skos:example "990577200"^^xsd:long .


###  https://mekanoid.io/ontology/extCategory
:extCategory rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :vendorProperty ;
             rdfs:label "category" .


###  https://mekanoid.io/ontology/extClassification
:extClassification rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :vendorProperty ;
                   rdfs:comment "An external classification that may apply to the artifact. Typically provided by a 3rd party vendor."@en ;
                   rdfs:label "classification"@en .


###  https://mekanoid.io/ontology/extPriority
:extPriority rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :extClassification ;
             rdf:type owl:FunctionalProperty ;
             rdfs:comment "Priority as defined by the 3rd party vendor responsible for creating the Artifact."@en ;
             rdfs:label "Priority"@en .


###  https://mekanoid.io/ontology/extRisk
:extRisk rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :extClassification ;
         rdfs:comment "Risk as defined by the 3rd party vendor responsible for creating the Artifact."@en ;
         rdfs:label "Risk"@en .


###  https://mekanoid.io/ontology/extSeverity
:extSeverity rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :extClassification ;
             rdf:type owl:FunctionalProperty ;
             rdfs:comment "Severity as defined by the 3rd party vendor responsible for creating the Artifact."@en ;
             rdfs:label "Severity"@en .


###  https://mekanoid.io/ontology/extStatus
:extStatus rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :vendorProperty ;
           rdfs:comment "Status of the Artifact as defined by the 3rd party vendor software responsible for generating it."@en ;
           rdfs:label "status"@en .


###  https://mekanoid.io/ontology/extThreat
:extThreat rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :extClassification ;
           rdfs:comment "Threat Level as defined by the 3rd party vendor responcible for creating the Artifact."@en ;
           rdfs:label "Threat Level"@en .


###  https://mekanoid.io/ontology/extType
:extType rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :vendorProperty ;
         rdfs:label "type" .


###  https://mekanoid.io/ontology/extUrgency
:extUrgency rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :extClassification ;
            rdf:type owl:FunctionalProperty ;
            rdfs:comment "Urgency as defined by the 3rd party vendor responsible for generating the Artifact."@en ;
            rdfs:label "Urgency"@en .


###  https://mekanoid.io/ontology/external-id
:external-id rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf d3f:identifier ,
                                :vendorProperty ;
             rdfs:label "external ID"@en ;
             skos:altLabel "vendor ID" ;
             skos:definition "Mekanoid data property that represents an identifier assigned by an external system or vendor. It is applicable to entities such as hosts and user accounts, providing a unique string-based identifier that aligns with external records or systems. This property facilitates the integration and mapping of internal entities with external references, enhancing cross-system data coherence and identification." .


###  https://mekanoid.io/ontology/firstSeen
:firstSeen rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :dateTime ,
                              :timeSeen ;
           rdf:type owl:FunctionalProperty ;
           rdfs:comment "The first time the enity had been observed by the application."@en ;
           rdfs:label "first seen"@en .


###  https://mekanoid.io/ontology/firstSeenEpoch
:firstSeenEpoch rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :dateTimeEpoch ;
                rdf:type owl:FunctionalProperty ;
                rdfs:label "first seen"@en .


###  https://mekanoid.io/ontology/hasURL
:hasURL rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf d3f:identifier .


###  https://mekanoid.io/ontology/id
:id rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf d3f:identifier ;
    rdfs:range xsd:string .


###  https://mekanoid.io/ontology/importBucketName
:importBucketName rdf:type owl:DatatypeProperty ;
                  rdfs:label "S3 bucket name"@en .


###  https://mekanoid.io/ontology/importBucketUrl
:importBucketUrl rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:range xsd:anyURI .


###  https://mekanoid.io/ontology/importFileName
:importFileName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:range xsd:anyURI ;
                rdfs:label "filename"@en .


###  https://mekanoid.io/ontology/importId
:importId rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf d3f:identifier ;
          rdfs:label "import ID"@en ;
          owl:deprecated "true"^^xsd:boolean .


###  https://mekanoid.io/ontology/lastAccessedEpoch
:lastAccessedEpoch rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :dateTimeEpoch ;
                   rdfs:label "last accessed"@en .


###  https://mekanoid.io/ontology/lastSeen
:lastSeen rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :dateTime ,
                             :timeSeen ;
          rdf:type owl:FunctionalProperty ;
          rdfs:comment "The last time the entity had been observed by the application."@en ;
          rdfs:label "last seen"@en .


###  https://mekanoid.io/ontology/lastSeenEpoch
:lastSeenEpoch rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :dateTimeEpoch ;
               rdf:type owl:FunctionalProperty ;
               rdfs:label "last seen"@en .


###  https://mekanoid.io/ontology/lastStatusChangeEpoch
:lastStatusChangeEpoch rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :dateTimeEpoch ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:label "last status change"@en .


###  https://mekanoid.io/ontology/lastUpdated
:lastUpdated rdf:type owl:DatatypeProperty ;
